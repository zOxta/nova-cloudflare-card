{
    "name": "zoxta/nova-cloudflare-card",
    "description": "Purge your CloudFlare's cache in two clicks from a Laravel Nova dashboard card.",
    "keywords": [
        "laravel",
        "nova",
        "cloudflate",
        "cache"
    ],
    "homepage": "https://github.com/zOxta/nova-cloudflare-card",
    "license": "MIT",
    "authors": [
        {
            "name": "zOxta",
            "email": "ezoxta@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.0.0 || ^7.0.0",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Zoxta\\NovaCloudflareCard\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Zoxta\\NovaCloudflareCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
